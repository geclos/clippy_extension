{
  "manifest_version": 2,
  "name": "Clippy",
  "description": "Clip interesting content from the internet",
  "version": "0.1",
  "permissions": ["tabs"],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+K",
        "mac": "Command+Shift+K"
      }
    }
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
